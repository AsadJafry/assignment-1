 Sub Main()
        Dim mid As Integer
        Dim mname As String
        FileOpen(1, "c:\pg\mdata.txt", OpenMode.Input)
        While Not EOF(1)

            Input(1, mid)
            Input(1, mname)

            Console.WriteLine("Member id" & mid)
            Console.WriteLine("Member name" & mname)


        End While
        FileClose(1)

        Console.ReadKey()



    End Sub
